<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_jetpack_ui"
    app:startDestination="@id/jetpackUIFragment">

    <fragment
        android:id="@+id/jetpackUIFragment"
        android:name="com.mokelab.demo.libs.jetpack.ui.JetpackUIFragment"
        android:label="JetpackUIFragment" >
        <action
            android:id="@+id/action_jetpack_ui_to_palette"
            app:destination="@id/paletteFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/paletteFragment"
        android:name="com.mokelab.demo.libs.jetpack.ui.palette.PaletteFragment"
        android:label="PaletteFragment"
        tools:layout="@layout/palette_fragment"/>
</navigation>